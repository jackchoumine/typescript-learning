{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": false,
    "semicolon": [
      false,
      "never"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-console": false,
    "trailing-comma": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": true,
    "no-empty": false,
    "no-shadowed-variable": false
  },
  "rulesDirectory": []
}